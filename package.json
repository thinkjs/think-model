{
  "name": "think-model",
  "version": "0.0.7",
  "description": "An adapter-based ORM for ThinkJS 3.x",
  "main": "./lib",
  "scripts": {
    "watch": "babel ./src --out-dir ./lib --watch",
    "compile": "babel ./src --out-dir ./lib",
    "test": "npm run eslint && npm run test-cov",
    "test-cov": "nyc ava test/ && nyc report --reporter=html",
    "eslint": "eslint ./src",
    "prepublish": "npm test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkjs/think-model.git"
  },
  "keywords": [
    "thinkjs",
    "orm",
    "mysql",
    "adapter",
    "model"
  ],
  "author": "lizheming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thinkjs/think-model/issues"
  },
  "homepage": "https://github.com/thinkjs/think-model#readme",
  "dependencies": {
    "ava": "^0.19.1",
    "think-cache": "^1.0.3",
    "think-debounce": "^1.0.3",
    "think-helper": "^1.0.5",
    "think-mysql": "github:thinkjs/think-mysql"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-think-node": "^1.0.1",
    "eslint": "^3.19.0",
    "muk": "^0.5.3",
    "nyc": "^10.3.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "think-node"
    ]
  }
}
